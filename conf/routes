# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET 	/apiv1/taxon/:id/geojson				controllers.TaxonController.showGeoJson(id: Long)

GET 	/apiv1/taxon/:id/informations			controllers.TaxonController.showInformations(id: Long)

GET		/apiv1/taxon/:id/parents				controllers.TaxonController.showParents(id: Long, limit: String ?= "KD")

GET		/apiv1/taxon/:id/childs 				controllers.TaxonController.showChilds(id: Long, q: String ?= "0")
