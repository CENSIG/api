# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page

GET 	/apiv1/taxon/:id/geojson		controllers.TaxonController.showGeoJson(id: Long)

GET 	/apiv1/taxon/:id/informations	controllers.TaxonController.showInformations(id: Long)

GET		/apiv1/taxon/:id/parents		controllers.TaxonController.showParents(id: Long)
